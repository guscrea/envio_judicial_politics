\slmult0\rtlch\afs24\ltrch\fs24{\rtlch\af2\alang1025\afs24\ltrch\b0\i0\fs18\lang1033\langnp1033\langfe1033\langfenp1033\loch\af2\dbch\af2\hich\f2\strike0\ulnone\cf1\super 4\~}{\*\bkmkstart Bookmark_fnpara_4}{\*\bkmkend Bookmark_fnpara_4}{\rtlch\af2\alang1025\afs24
